{
    "name": "echo",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "deploy": "wrangler deploy",
        "dev": "wrangler dev",
        "start": "wrangler dev",
        "build": "npm run handlebars",
        "cf-typegen": "wrangler types",
        "handlebars": "mkdir generated && npm run compilehbs && npm run transpilehbs",
        "compilehbs": "handlebars -f generated/template.cjs src/template.html",
        "transpilehbs": "hbs-import-transpile generated/template.cjs > generated/template.mjs"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20250124.3",
        "typescript": "^5.5.2",
        "wrangler": "^3.105.1",
        "hbs-import-transpile": "^1.0.4"
    },
    "dependencies": {
        "handlebars": "^4.7.8",
        "hbs-async-render": "^1.0.0"
    }
}
